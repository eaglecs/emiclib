<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="100dip" />

            <gradient
                android:angle="270"
                android:centerColor="@color/md_grey700"
                android:centerY="0.75"
                android:endColor="@color/md_grey600"
                android:startColor="@color/md_grey300" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="100dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/md_grey700"
                    android:centerY="0.75"
                    android:endColor="@color/md_grey600"
                    android:startColor="@color/md_grey300" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="100dip" />
                <gradient
                    android:angle="270"
                    android:endColor="#76b435"
                    android:startColor="@color/material_lightgreenA700" />
            </shape>
        </clip>
    </item>

</layer-list>